<h1>ユーザー一覧</h1>

<table>
  <thead>
    <tr>
      <th>ユーザーID</th>
      <th>ユーザーネーム</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.is_active %></td>
    </tr>
    <% end %> 
  </tbody>
</table>



