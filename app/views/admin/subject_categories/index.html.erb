<h1>教科一覧</h1>
<%= form_with model: [:admin, @subject_category] do |f| %>
  <label for="InputSubject">教科名</label><br>
  <%= f.text_field :name, palceholder:"教科名" %><br>
  <%= f.submit "教科を登録" %>
<% end %>

<table>
 <thead>
  <tr>
   <th>教科名</th>
  </tr>
 </thead>
 <tbody>
  <% @subject_categories.each do |subject_category| %>
  <tr>
   <td><%= subject_category.name %></td>
   <td><%= link_to "編集", edit_admin_subject_category_path(subject_category) %></td>
  </tr>
  <% end %>
 </tbody>
</table>